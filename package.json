{
  "private": true,
  "main": "package.json",
  "name": "sanity-gatsby-blog",
  "version": "1.0.0",
  "scripts": {
    "build": "lerna run build --parallel",
    "dev": "lerna run dev --parallel",
    "format": "lerna run format",
    "build-studio": "cd studio && npm run build",
    "build-web": "(cd studio && SANITY_AUTH_TOKEN=$SANITY_DEPLOY_STUDIO_TOKEN npm run graphql-deploy) && (cd web && npm run build)",
    "graphql-deploy": "lerna run graphql-deploy",
    "lint": "lerna run lint",
    "postinstall": "lerna bootstrap",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@sanity/base": "0.134.0",
    "@sanity/components": "0.134.0",
    "@sanity/core": "0.134.0",
    "@sanity/default-layout": "0.134.0",
    "@sanity/default-login": "0.134.0",
    "@sanity/desk-tool": "0.134.0",
    "@sanity/vision": "0.134.0",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-icons": "3.1.0"
  },
  "devDependencies": {
    "@sanity/cli": "^0.140.17",
    "lerna": "^3.13.1"
  }
}
